! Colours: 
! Uses the University of Cambridge colour palette.

#define UC_Red       #d6083b
#define UC_BRed      #901c3b
#define UC_Yellow    #ea7125
#define UC_BYellow   #cb4f00
#define UC_Green     #55a51c
#define UC_BGreen    #445026
#define UC_Blue      #0072cf
#define UC_BBlue     #003e74
#define UC_Magenta   #8f2bbc
#define UC_BMagenta  #422e5d
#define UC_Cyan      #00b1c1
#define UC_BCyan     #106470
#define UC_Black     #1b1f20
#define UC_BBlack    #2e3436
#define UC_White     #abadae
#define UC_BWhite    #c0c2c2

! URxvt:

URxvt*intensityStyles: true
URxvt*fading: 40
URxvt*fadeColor: UC_BBlack

*background: UC_Black
*foreground: UC_White
*cursorColor: UC_BWhite
*pointerColorBackground: UC_BGreen
*pointerColorForeground: UC_BCyan

! Black & Dark Grey
*color0: UC_Black
*color8: UC_BBlack
! Red and Orange
*color1: UC_Red
*color9: UC_BRed
! Green & DarkGreen
*color2: UC_Green
*color10: UC_BGreen
! Yellow & DarkYellow
*color3: UC_Yellow
*color11: UC_BYellow
! Blue & DarkBlue
*color4: UC_Blue
*color12: UC_BBlue
! Magenta & Violet
*color5: UC_Magenta
*color13: UC_BMagenta
! Cyan & DarkCyan
*color6: UC_Cyan
*color14: UC_BCyan
! LightGrey & White
*color7: UC_White
*color15: UC_BWhite
